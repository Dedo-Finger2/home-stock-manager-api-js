{
  "name": "home-stock-manager-api-js",
  "version": "1.0.0",
  "description": "Home Stock Manager",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:staged": "jest --testPathPattern src/**/test/unit/*.unit.test.js --passWithNoTests",
    "test:unit": "jest --testPathPattern src/**/test/unit/*.unit.test.js",
    "test:unit:watch": "jest --testPathPattern --watch src/**/test/unit/*.unit.test.js",
    "test:integration": "jest --testPathPattern src/**/test/integration/*.integration.test.js",
    "test:integration:watch": "jest --testPathPattern --watch src/**/test/integration/*.integration.test.js",
    "test:e2e": "jest --testPathPattern src/**/test/e2e/*.e2e.test.js",
    "test:e2e:watch": "jest --testPathPattern --watch src/**/test/e2e/*.e2e.test.js",
    "dev": "node --watch --env-file .env src/server.js",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Antonio Mauricio",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "9.7.0",
    "@swc/core": "1.7.1",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "eslint": "9.7.0",
    "globals": "15.8.0",
    "husky": "9.1.1",
    "jest": "29.7.0",
    "lint-staged": "15.2.7",
    "prettier": "3.3.3"
  },
  "dependencies": {
    "fastify": "4.28.1",
    "valibot": "0.36.0"
  }
}
